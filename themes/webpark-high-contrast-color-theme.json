{
	"$schema": "vscode://schemas/color-theme",
	"name": "Webpark High Contrast",
	"colors": {
		// Editor Colors
		"editor.background": "#000000",
		"editor.foreground": "#ffffff",
		"editorWhitespace.foreground": "#404040",

		// Activity Bar
		"activityBar.background": "#000000",
		"activityBar.foreground": "#ffffff",
		"activityBarBadge.background": "#ffff00",

		// Side Bar
		"sideBar.background": "#000000",
		"sideBarTitle.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#1e1e1e",

		// Tabs
		"tab.activeBackground": "#000000",
		"tab.inactiveBackground": "#1e1e1e",
		"tab.border": "#ffffff",
		"editorGroupHeader.tabsBackground": "#1e1e1e",

		// Status Bar
		"statusBar.background": "#007acc",
		"statusBar.foreground": "#ffffff",

		// Title Bar
		"titleBar.activeBackground": "#000000",

		// Panels and Quick Input
		"panel.background": "#000000",
		"quickInput.background": "#000000",
		"quickInputTitle.background": "#000000",

		// Dropdowns
		"dropdown.background": "#000000",
		"dropdown.border": "#ffffff",

		// Hover Widgets
		"editorHoverWidget.background": "#000000",
		"editorHoverWidget.border": "#ffffff",

		// Suggest Widgets
		"editorSuggestWidget.background": "#000000",
		"editorSuggestWidget.border": "#ffffff",
		"editorSuggestWidget.selectedBackground": "#007acc",

		// Peek View
		"peekView.border": "#ffff00",
		"peekViewEditor.background": "#000000",
		"peekViewEditor.matchHighlightBackground": "#ffff00",
		"peekViewTitle.background": "#000000",

		// Menu
		"menu.border": "#ffffff",

		// Editor Gutter
		"editorGutter.background": "#000000",
		"editorGutter.modifiedBackground": "#ffff00",
		"editorGutter.addedBackground": "#00ff00",
		"editorGutter.deletedBackground": "#ff0000",

		// Editor Errors, Warnings, Info, and Hints
		"editorError.foreground": "#ff0000",
		"editorWarning.foreground": "#ffff00",
		"editorInfo.foreground": "#00ffff",
		"editorHint.foreground": "#00ffff",

		// Overview Ruler
		"editorOverviewRuler.errorForeground": "#ff0000",
		"editorOverviewRuler.warningForeground": "#ffff00",
		"editorOverviewRuler.infoForeground": "#00ffff",
		"editorOverviewRuler.bracketMatchForeground": "#ffff00",
		"editorOverviewRuler.commonContentForeground": "#404040",
		"editorOverviewRuler.incomingContentForeground": "#ffff00",
		"editorOverviewRuler.currentContentForeground": "#00ffff"
	},
	  
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00ff00"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#00ff00"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language",
				"constant.other"
			],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		}
	]
}
