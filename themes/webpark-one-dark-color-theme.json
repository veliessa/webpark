{
	"$schema": "vscode://schemas/color-theme",
	"name": "Webpark One Dark",
	"colors": {
		// Editor Colors
		"editor.background": "#282c34",
		"editor.foreground": "#abb2bf",
		"editorWhitespace.foreground": "#3e4451",

		// Activity Bar
		"activityBar.background": "#282c34",
		"activityBar.foreground": "#abb2bf",
		"activityBarBadge.background": "#61afef",

		// Side Bar
		"sideBar.background": "#21252b",
		"sideBarTitle.foreground": "#abb2bf",
		"sideBarSectionHeader.background": "#3e4451",

		// Tabs
		"tab.activeBackground": "#282c34",
		"tab.inactiveBackground": "#21252b",
		"tab.border": "#3e4451",
		"editorGroupHeader.tabsBackground": "#21252b",

		// Status Bar
		"statusBar.background": "#61afef",
		"statusBar.foreground": "#ffffff",

		// Title Bar
		"titleBar.activeBackground": "#21252b",

		// Panels and Quick Input
		"panel.background": "#21252b",
		"quickInput.background": "#3e4451",
		"quickInputTitle.background": "#3e4451",

		// Dropdowns
		"dropdown.background": "#3e4451",
		"dropdown.border": "#5c6370",

		// Hover Widgets
		"editorHoverWidget.background": "#3e4451",
		"editorHoverWidget.border": "#5c6370",

		// Suggest Widgets
		"editorSuggestWidget.background": "#3e4451",
		"editorSuggestWidget.border": "#5c6370",
		"editorSuggestWidget.selectedBackground": "#4b5263",

		// Peek View
		"peekView.border": "#61afef",
		"peekViewEditor.background": "#21252b",
		"peekViewEditor.matchHighlightBackground": "#61afef",
		"peekViewTitle.background": "#3e4451",

		// Menu
		"menu.border": "#5c6370",

		// Editor Gutter
		"editorGutter.background": "#282c34",
		"editorGutter.modifiedBackground": "#e5c07b",
		"editorGutter.addedBackground": "#98c379",
		"editorGutter.deletedBackground": "#e06c75",

		// Editor Errors, Warnings, Info, and Hints
		"editorError.foreground": "#e06c75",
		"editorWarning.foreground": "#e5c07b",
		"editorInfo.foreground": "#61afef",
		"editorHint.foreground": "#98c379",

		// Overview Ruler
		"editorOverviewRuler.errorForeground": "#e06c75",
		"editorOverviewRuler.warningForeground": "#e5c07b",
		"editorOverviewRuler.infoForeground": "#61afef",
		"editorOverviewRuler.bracketMatchForeground": "#61afef",
		"editorOverviewRuler.commonContentForeground": "#5c6370",
		"editorOverviewRuler.incomingContentForeground": "#e5c07b",
		"editorOverviewRuler.currentContentForeground": "#61afef"
	},
	  
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5c6370"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language",
				"constant.other"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#abb2bf"
			}
		}
	]
}
